  <body id="body">
      <nav class="navbar">
        <div class="logo">
          <h3>DevlinB.</h3>
        </div>
        <ul>
          <li><a href="#projects_container">projects</a></li>
          <li><a href="#about">about</a></li>
          <li><a href="#contact">contact</a></li>
        </ul>
      </nav>
      <div id="main">
        <div id="intro">
          <img id="profile_Image" src="images/profilePic2.jpeg" alt="profilePic">
          <h1>Hi, I'm Devlin!</h1>
          <h3>Full Stack Software Developer & Designer</h5>
            <div class="row">
              <div class="connect_box">
                <a class="nav-a" href="https://github.com/adevlinb" target="_blank">github</a>
              </div>
              <div class="connect_box">
                <a class="nav-a" href="https://www.linkedin.com/in/devlin-booth/" target="_blank">linkedin</a>
              </div>
              <div class="connect_box">
                <a class="nav-a" href="images/Devlin Booth Resume 5:12:22.pdf" target="_blank">resume</a>
              </div>
            </div>
        </div>
